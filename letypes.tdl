v_impers_intr_io_mrkd_vp_inf_synsem := v_impers_intr_io_mrkd_np-or-prop_synsem &
  [ LOCAL [ CAT.VAL.COMPS < synsem,
                            [ LOCAL mrkd_cp_prop_inf_local &
                                    [ CAT.MC -,
                                      CONT.HOOK [ LTOP #larg,
