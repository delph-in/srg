  """.


p_ap_i-vm_lex := mod-adp-lex & p_ap & norm-sem-lex-item &
  [ SYNSEM p_vmod_synsem & [ LOCAL.CAT [ HEAD.MOD < [ LOCAL.CONT.HOOK.XARG #ctrl ] >,
