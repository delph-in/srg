v_impers_intr_mrkd_np_synsem := v_impers_intr_mrkd_np-or-prop_synsem &
  [ LOCAL [ CAT.VAL.COMPS < [ LOCAL mrkd_np_local & 
                                    [ CAT.HEAD.KEYS.ALTKEY non_temp_nonpro_rel,
