basic-adjective-lex := lex-item &
  [ INFLECTED -,
    ALTS [ RCP -, 
           RFX -,
           PASS -,
           IMPERS - ],
    SYNSEM [ PUNCT [ LPUNCT no_punct,
                     RPUNCT no_punct ],
             MODIFIED notmod,
             LOCAL [ COORD -,
                     COORD-STRAT zero,
                     COORD-REL.PRED null_coord_rel,
                     CAT [ HEAD adj & 
                                [ KEYS.KEY basic_adj_rel ],
                           VAL [ SUBJ < >,
                                 CLTS < > ] ] ] ] ].

basic-ord-num-lex := lex-item &
  [ INFLECTED -,
    ALTS [ RCP -, 
           RFX -,
           PASS -,
           IMPERS - ],
    SYNSEM [ PUNCT [ LPUNCT no_punct,
                     RPUNCT no_punct ],
             MODIFIED notmod,
             LOCAL [ COORD -,
                     COORD-STRAT zero,
                     COORD-REL.PRED null_coord_rel,
                     CAT [ HEAD adj & 
                                [ KEYS.KEY ord_rel ],
                           VAL [ SUBJ < >,
                                 CLTS < > ] ] ] ] ].


non-agr_basic_adjective_synsem := lex-synsem &
  [ LOCAL [ AGR.PNG.GEN #gen,
            CAT [ MC na,
                  HEAD [ KEYS.KEY basic_adj_rel,
